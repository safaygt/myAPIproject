<!DOCTYPE html>
<html>
<head>
    <title>Top Artists</title>
</head>
<body>
    <h1>Sanatçılar</h1>

    <% if (Array.isArray(artists) && artists.length > 0) { %> <!-- Dizi ve uzunluğu kontrol ediliyor -->
        <ul>
            <% artists.forEach(artist => { %>
                <li><%= artist %></li>
            <% }); %>
        </ul>
    <% } else { %>
        <p>Veriler bekleniyor.</p>
    <% } %>

    <form id="myForm" method="post">
        <input id="get" type="submit" value="Veri Getir" formaction="/get-data">
    </form>
</body>
</html>